{
    "manifest_version": 3,
    "name": "Scrapper",
    "version": "1.1.0",
    "action": {
        "default_title": "Click to Scrap",
        "default_popup":"popup.html"
    },
    "background": {
        "service_worker": "sw.js",
        "type": "module"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "notifications",
        "scripting"
    ]
}